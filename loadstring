-- Module Scripts

local fake_module_scripts = {}

do -- MainFrame.ZeroLibrary
	local script = Instance.new('ModuleScript', MainFrame)
	script.Name = "ZeroLibrary"
	local function module_script()
		local ZeroDay = script.Parent
		
		local TweenService = game:GetService("TweenService")
		local UIS = game:GetService("UserInputService")
		
		
		
		UIS.InputBegan:Connect(function(input)
			if input.KeyCode == Enum.KeyCode.V then
				script.Parent.Visible = not script.Parent.Visible
			end
		end)
		
		local ZeroLibrary = {}
		
		
		
		
		function ZeroLibrary:Notify(title, text, duration)
			local NotificationTemp = script.Parent.Stacker.Notifications:Clone()
			NotificationTemp.Thingy.Text = title
			NotificationTemp.Thingy2.Text = text
			NotificationTemp.Visible = true
			
			
			local tweenInfo = TweenService:Create(NotificationTemp, TweenInfo.new(1.8, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {BackgroundTransparency = 0})
			local tweenInfo2 = TweenService:Create(NotificationTemp.Thingy, TweenInfo.new(1.8, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {TextTransparency = 0})
			local tweenInfo3 = TweenService:Create(NotificationTemp.Thingy2, TweenInfo.new(1.8, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {TextTransparency = 0})
			tweenInfo:Play()
			tweenInfo2:Play()
			tweenInfo3:Play()
			wait(duration)
			NotificationTemp:Destroy()
		end
		
		function ZeroLibrary:CreateButton(name,window,callback)
			local ButtonTemp = ZeroDay.ButtonTemp:Clone()
			ButtonTemp.Visible = true
			local Options = {Enabled = false}
			local Arraylist = ZeroDay.Arraylist
			local Temp = Arraylist.Template:Clone()
			
			Temp.Visible = true
			Temp.Name = name
			Temp.Text = name
		
			
			if window == 'Blatant' then
				ButtonTemp.Parent = ZeroDay.Blatant.Window
			elseif window == 'World' then
				ButtonTemp.Parent = ZeroDay.World.Window
			elseif window == 'Visual' then
				ButtonTemp.Parent = ZeroDay.Visual.Window
			elseif window == 'Combat' then
				ButtonTemp.Parent = ZeroDay.Combat.Window
			end
			ButtonTemp.MouseButton1Click:Connect(function()
				
				
				
				
				if Options.Enabled == false then
					if callback then
						callback()
					end
					
					Options.Enabled = true
					ButtonTemp.BackgroundTransparency = 0
				elseif Options.Enabled == true then
					Temp:Destroy()
					Options.Enabled = false
					ButtonTemp.BackgroundTransparency = 1
				end
			end)
			ButtonTemp.Text = name
			ButtonTemp.Visible = true
		end
		return ZeroLibrary
	end
	fake_module_scripts[script] = module_script
end
do -- Notifications.Testy
	local script = Instance.new('ModuleScript', Notifications)
	script.Name = "Testy"
	local function module_script()
		local ZeroLibrary = {}
		
		function ZeroLibrary:Notify(title, text, duration)
			local NotificationTemp = script.Parent:Clone()
			NotificationTemp.Thingy.Text = title
			NotificationTemp.Thingy2.Text = text
			NotificationTemp.Visible = true
		
			local TS = game:GetService("TweenService")
			
			NotificationTemp:TweenSize(UDim2.new(0, 232, 0, 63), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 0.4)
			wait(0.4)
			NotificationTemp:TweenSize(UDim2.new(0, 4, 0, 63), Enum.EasingDirection.In, Enum.EasingStyle.Sine, 0.4)
			wait(duration)
			NotificationTemp:Destroy()
		end
		
		return ZeroLibrary
	end
	fake_module_scripts[script] = module_script
end


-- Scripts

local function QDWOG_fake_script() -- MainFrame.Handler 
	local script = Instance.new('LocalScript', MainFrame)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local Library = require(script.Parent.ZeroLibrary)
	local UIS = game:GetService("UserInputService")
	
	Library:CreateButton('Aura','Blatant', function(callback)
		print("hello world")
		local hi = "hi"
		print(hi)
	end)
	
end
coroutine.wrap(QDWOG_fake_script)()
local function DXWIC_fake_script() -- World.Smooth GUI Dragging 
	local script = Instance.new('LocalScript', World)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
end
coroutine.wrap(DXWIC_fake_script)()
local function PRRCEI_fake_script() -- Blatant.Smooth GUI Dragging 
	local script = Instance.new('LocalScript', Blatant)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
